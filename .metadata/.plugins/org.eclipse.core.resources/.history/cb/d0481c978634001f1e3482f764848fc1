package projectOne;
//import java.util.Scanner;
public class dev047 
{
//	public static Scanner leia = new Scanner(System.in);
	public static void main(String[] args) 
	{
		int contador = 0, numero, x;

		do
		{
			contador = contador + 1;
			x = numero*contador;
			System.out.print(""+numero+"x"+contador+"="+x+"\n");
		}
		while(contador < 10);
	}

}
